<IfModule mod_expires.c>

ExpiresActive On
ExpiresDefault "access plus 1 month"

ExpiresByType image/jpg "access plus 2 weeks"
ExpiresByType image/gif "access plus 2 weeks"
ExpiresByType image/png "access plus 2 weeks"

ExpiresByType font/ttf "access plus 2 weeks"
ExpiresByType font/otf "access plus 2 weeks"
ExpiresByType font/woff "access plus 2 weeks"
ExpiresByType font/woff2 "access plus 2 weeks"
ExpiresByType application/font-woff "access plus 2 weeks"



ExpiresByType text/css "access plus 2 weeks"
ExpiresByType text/javascript "access plus 2 weeks"
ExpiresByType image/x-icon "access plus 2 weeks"



</IfModule>